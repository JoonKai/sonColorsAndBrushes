<Window x:Class="sonColorsAndBrushes.CustomColormapBrushes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sonColorsAndBrushes"
        mc:Ignorable="d"
        Title="CustomColormapBrushes" Height="365" Width="300">
    <Grid Margin="0" x:Name="chartGrid" ClipToBounds="False" Background="Transparent" SizeChanged="chartGrid_SizeChanged">
        <Canvas Margin="10" x:Name="chartCanvas" ClipToBounds="False" Background="#66CCCCCC"/>
    </Grid>
</Window>
